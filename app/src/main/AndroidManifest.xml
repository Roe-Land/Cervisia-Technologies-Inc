<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dennis.studlife">

    <application
        android:name=".ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CollegeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".HighscoresActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".UitgaanActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".GameOverActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ThuisActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Events"
            android:screenOrientation="portrait"/>
    </application>

</manifest>